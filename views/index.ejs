		crossorigin="anonymous"></script>

	<script src="/public/js/main.js"></script>
	<script src="/public/js/mainpwa.js"></script>

</head>

<body class="fullscreen">


	<div class="container" id="reportpage">
		<div class="row justify-content-center">
			<!-- <div class="col-md-6 text-center mb-5">
					<h2 class="heading-section">AutoCheck Report </h2> <a
						href="/public/report/sample_autocheck_report.html" target="_blank" style="color:blue">See a
						sample</a>
				</div> -->
		</div>

		<div class="row ">

			<div class="wrap" id="main-content">
				<div id="left-slider">

				</div>

				<div class="w3-teal" style="width:100%">
					<button class="w3-button w3-teal w3-xlarge" onclick="w3_open()"><span
							style="font-size:38px">&#9776;</span></button>

					<h3 class="mb-4" style="display:inline; color:white;font-size:18px" id="pageTitle"><%= `${user.firstname} ${user.lastname}` %></h3>
					<img id="app-logo" src="/public/images/logo.jpg"
						style="position: absolute;    top: 5px;    right: 0; height:60px" />
				</div>
				<!-- <div class="img" style="background-image: url(/public/images/bg-1.jpg);"></div> -->


				<!-- <div >
              <a href="#test">
                <img src="/public/images/therapy/back.png" width="100" />
           </a>
            </div> -->

				<div class="login-wrap p-4 p-md-5 justify-content-center" style="padding-top:0px !important" onclick="w3_close()">
					<!-- <a href="/public/html/chat.html"> <img  style="float:right" src="/public/images/live-chat.jpg"/></a> -->
					<div>
						<input type="hidden" id="patient_app_id" value="" placeholder="APP ID"
							style="width:100%;margin-bottom:20px">
					</div>

					<div>
						<a href="/public/html/dsa.html" >
							<button class="btn btn-light rounded my-2">
								<img src="/public/images/therapy/dsa.jpg" width="100" height="110" alt="back"
									style="position:relative;left:-25px" /> Daily Sleep Assessment
							</button></a>
					</div>

					<div>
						<a href="/public/html/general-notes.html" >
							<button class="btn btn-light rounded my-2">
								<img src="/public/images/therapy/general-notes.jpg"  width="100"
									height="110" alt="back" style="position:relative;left:-45px" /> General Notes
							</button></a>
					</div>

					<div>
						<a href="/public/html/therapy-tracker.html">
							<button class="btn btn-light rounded my-2">
								<img src="/public/images/therapy/therapy-tracker-all.jpg" 
									height="110" alt="back"/>
							</button></a>
					</div>

					<div>
						<a href="/public/html/event-tracker.html" >
							<button class="btn btn-light rounded my-2">
								<img src="/public/images/event-tracker.jpg"  width="100"
									height="110" alt="back" style="position:relative;left:-45px" /> Event Tracker
							</button></a>
					</div>

					<div>
						<a href="/public/html/mydata.html" >
							<button class="btn btn-light rounded my-2">
								<img src="/public/images/chart.jpg"  width="100"
									height="110" alt="back" style="position:relative;left:-45px" /> My Data
							</button></a>
					</div>

					<!-- Button trigger modal -->
					<button type="button" id="appUpdateBtn" class="btn btn-primary" data-toggle="modal"
						data-target="#exampleModal" style="display:none">
						Show App Update Modal
					</button>
<!-- 
					<button type="button" id="activateBtn" class="btn btn-primary" data-toggle="modal"
						data-target="#activateModal" style="display:none">
						Activate App
					</button> -->

					<input id="app-version" type="hidden" value="2.2">

				</div>
			</div>
		</div>
	</div>


	<!-- Modal -->
	<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
		aria-hidden="true">
		<div class="modal-dialog" role="document">
			<div class="modal-content">
				<div class="modal-header">
					<h5 class="modal-title" id="exampleModalLabel">New Update Availlable</h5>
					<button type="button" class="close" data-dismiss="modal" aria-label="Close">
						<span aria-hidden="true">&times;</span>
					</button>
				</div>
				<div class="modal-body" id="appversion-message">

				</div>
				<div class="modal-footer">
					<button type="button" class="btn btn-secondary" data-dismiss="modal" id="closeAppUpdate"
						style="display:none">OK</button>
					<a href="https://csmaapp.azurewebsites.net/" target="_blank" rel="noopener"><button type="button"
							class="btn btn-primary" onclick='document.getElementById("closeAppUpdate").click()'>Update
							new app</button></a>
				</div>
			</div>
		</div>
	</div>

	<div class="modal fade" id="activateModal" tabindex="-1" role="dialog" aria-labelledby="activateModalLabel"
		aria-hidden="true">
		<div class="modal-dialog" role="document">
			<div class="modal-content">
				<div class="modal-header">
					<h5 class="modal-title" id="activateModalLabel">Activate your Profile! </h5>
					<button type="button" class="close" data-dismiss="modal" aria-label="Close">
						<span aria-hidden="true">&times;</span>
					</button>
				</div>
				<div class="modal-body" id="">
					Your App ID is inactive. Please activate your profile by clicking the link sent to you via email.
				</div>
				<div class="modal-footer">
					<button type="button" class="btn btn-secondary" data-dismiss="modal"
						id="closeAppInstall">OK</button>
					<!-- <a href="https://csmaapp.azurewebsites.net/" target="_blank" rel="noopener"><button type="button" class="btn btn-primary" onclick='document.getElementById("closeAppUpdate").click()'>Update new app</button></a>  -->
				</div>
			</div>
		</div>
	</div>


	<button type="button" id="activatedBtn" class="btn btn-primary" data-toggle="modal" data-target="#activatedModal"
		style="display:none">
		Activated App
	</button>

	<div class="modal fade" id="activatedModal" tabindex="-1" role="dialog" aria-labelledby="activatedModalLabel"
		aria-hidden="true">
		<div class="modal-dialog" role="document">
			<div class="modal-content">
				<div class="modal-header" style="background-color:lightgreen">
					<h5 class="modal-title" id="activatedModalLabel" style="text-align: center;font-weight: bold;">Your
						Profile is Activated!</h5>
					<button type="button" class="close" data-dismiss="modal" aria-label="Close">
						<span aria-hidden="true">&times;</span>
					</button>
				</div>

				<div class="modal-footer">
					<!-- <button type="button" class="btn btn-secondary" ><a href="/public/html/settings.html"> OK</a></button> -->
					<button type="button" class="btn btn-secondary" data-dismiss="modal"> OK</button>
					<!-- <a href="https://csmaapp.azurewebsites.net/" target="_blank" rel="noopener"><button type="button" class="btn btn-primary" onclick='document.getElementById("closeAppUpdate").click()'>Update new app</button></a>  -->
				</div>
			</div>
		</div>
	</div>

	<script>
const user = <%- JSON.stringify(user) %>
localStorage.setItem("rest-tracker-app-profile", JSON.stringify(user))

// localStorage.getItem("rest-tracker-app-profile") 

</script>

</body>


</html>