<body class="fullscreen">


	<div class="container" id="reportpage">
		<div class="row justify-content-center">
			<!-- <div class="col-md-6 text-center mb-5">
					<h2 class="heading-section">AutoCheck Report </h2> <a
						href="/public/report/sample_autocheck_report.html" target="_blank" style="color:blue">See a
						sample</a>
				</div> -->
		</div>

		<div class="row ">


			<div class="wrap">

				<div id="left-slider"></div>

				<div class="w3-teal" style="width:100%">
					<button class="w3-button w3-teal w3-xlarge" onclick="w3_open()"><span
							style="font-size:38px">&#9776;</span></button>

					<h3 class="mb-4" style="display:inline; color:white;" id="pageTitle">Settings</h3>
					<img id="app-logo" src="/public/images/logo.jpg" style="position: absolute;    top: 5px;    right: 0; height:60px" />
				</div>
				<!-- <div class="img" style="background-image: url(/public/images/bg-1.jpg);"></div> -->


				<!-- <div >
              <a href="#test">
                <img src="/public/images/therapy/back.png" width="100" />
           </a>
            </div> -->

				<a href="/public/html/index.html" class="back-btn">
					<img src="/public/images/therapy/back.png" alt="back" width="50" />
				</a>

				<div class="login-wrap p-4 p-md-5 justify-content-center" onclick="w3_close()">


					<div>
						<h3>App Version: 2.2</h3>
					</div>


					<form action="/change-password" class="signin-form" method="post"
						onsubmit="return validateConfirmPassword()">


						<div class="form-group">
							<label  for="patient_app_id">App ID</label>
							<input type="text" id="patient_app_id" name="patient_app_id" class="form-control"  required>
							<button type="button" class="form-control btn btn-primary rounded submit px-3" onclick="saveAppId()">Save App Id</button>
							
						</div>
						<br/>

						<div class="form-group">
							<input type="password" class="form-control" id="currentpassword" name="currentpassword"	required>
							<label class="form-control-placeholder" for="currentpassword">Current Password</label>
						</div>

						<div class="form-group">
							<input type="password" class="form-control" id="newpassword" name="newpassword"
								pattern="^(?=.*[a-z])(?=.*[A-Z])(?=.*\d)[a-zA-Z\d]{8,}$|^(?=.*[A-Za-z])(?=.*\d)(?=.*[$@$!%*#?&])[A-Za-z\d$@$!%*#?&]{8,}$"
								title="Minimum eight characters, at least one uppercase letter, one lowercase letter and one number"
								required>
							<label class="form-control-placeholder" for="newpassword">New Password</label>
							<span toggle="#newpassword" class="fa fa-fw fa-eye field-icon toggle-password"></span>
						</div>

					<div class="form-group">
						<input type="password" class="form-control" id="confirm_password" name="confirm_password"
							required>
						<label class="form-control-placeholder" for="confirm_password">Confirm Password</label>
					</div>

					<div class="form-group">
						<button type="submit" class="form-control btn btn-primary rounded submit px-3">Change Password</button>
					</div>
				</form>
				<div class="form-group">
					<span id="note" style="color:red; font-weight: bold;"><%- message %></span>
				</div>


					
				</div>

			</div>


		</div>
	</div>

</body>

</html>